{
  "name": "r101-negapollo",
  "version": "0.0.1",
  "description": "An app to dark launch graphql changes",
  "main": "index.js",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-prettier": "^2.1.2",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.2",
    "prettier-check": "^2.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "src",
    "setupTestFrameworkScriptFile": "./setupTestFrameworkScript.js",
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 100
  },
  "scripts": {
    "start": "NODE_PATH=src/ nodemon src/server.js",
    "dev": "NODE_ENV=development DEBUG=\"API,error\" yarn start",
    "test": "jest --forceExit",
    "test-watch": "jest --watch",
    "lint": "./node_modules/.bin/eslint src --ext=.js --quiet",
    "format": "./node_modules/.bin/prettier --write \"src/**/*.js\"",
    "prettier:check": "./node_modules/.bin/prettier-check \"src/**/*.js\"",
    "precommit": "lint-staged",
    "prepush": "./node_modules/.bin/eslint src --ext=.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remind101/r101-negapollo.git"
  },
  "keywords": [
    "graphql",
    "rollout"
  ],
  "author": "Sophie Dawson",
  "bugs": {
    "url": "https://github.com/remind101/r101-negapollo/issues"
  },
  "homepage": "https://github.com/remind101/r101-negapollo#readme",
  "resolutions": {
    "eslint-scope": "3.7.1"
  }
}
